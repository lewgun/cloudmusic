{
  "name": "cloudmusic",
  "version": "1.0.0",
  "description": "a classroom for learning go and ng2",
  "main": "index.js",
  "dependencies": {
    "angular2": "latest",
    "big-integer": "^1.6.12",
    "es6-promise": "latest",
    "es6-shim": "latest",
    "lite-server": "latest",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "latest",
    "typings": "latest",
    "zone.js": "latest"
  },
  "devDependencies": {},
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "browserify": "browserify src/frontend/boot.js -o src/frontend/app.js -s app",
    "uglify": "uglifyjs src/frontend/app.js -m -o src/frontend/app.min.js",
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\" ",
    "all": "concurrent \"npm run tsc\"  \"npm run browserify\" \"npm run lite\" \"npm run uglify\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lewgun/cloudmusic.git"
